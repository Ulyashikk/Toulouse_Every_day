from webparcingtest import parse_and_update_data
import schedule
import time

# Запланируйте выполнение функции parse_and_update_data каждый день в определенное время
schedule.every().day.at("09:00").do(parse_and_update_data)

# Запуск бесконечного цикла для выполнения расписания
while True:
    schedule.run_pending()
    time.sleep(1)